
<%= form_for @soupkitchen do |f| %>


    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    
    <%= f.label :address %>
    <%= f.text_field :address %>
    <br>


    <%= f.label :city %>
    <%= f.text_field :city %>
    <br>
    	
    <%= f.label :state %>
    <%= f.text_field :state %>
    <br>


    <%= f.label :zipcode %>
    <%= f.text_field :zipcode %>
    <br>

    <%= f.label :phone %>
    <%= f.text_field :phone %>
    <br>

    <%= f.label :indoors?  %>
    <%= f.check_box :indoors, :checked => true %>
    <br>

    		

    <%= f.submit %>
<% end %>